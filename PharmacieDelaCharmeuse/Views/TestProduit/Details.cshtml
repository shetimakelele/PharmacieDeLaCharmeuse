@model PharmacieDeLaCharmeuse.Core.Data.Models.TestProduit

<section id="gammes">
    <div class="container heading">
        <h4>@Model.Titre</h4>
    </div>
    <div class="divider"></div>


    <div class="container">
        <div class="row">

            <div class="col-md-6">
                <img src="@Model.TestImageUrl" alt="" class="img-thumbnail" />
            </div>
            <div class="col-md-6">
                <h3 class="my-3"> Description</h3>
                <h4 class="text-center">Note : @Model.NoteTest / 10</h4>
                <hr />
                <p>@Model.Description.</p>


            </div>

        </div>
    </div>

</section>

